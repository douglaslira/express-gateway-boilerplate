{
	"name": "express-gateway-boilerplate",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"clean": "rimraf dist",
		"copy-deps": "copy-newer --cwd node_modules ** ./dist/node_modules",
		"start": "node dist/server/server.js",
		"preserve": "npm run build",
		"s": "npm run serve",
		"serve.w": "concurrently \"npm run build.w\" \"nodemon --watch ./src ./dist/server ./dist/server/server.js\"",
		"serve": "npm run start",
		"lint": "tslint \"src/**/*.ts\"",
		"prebuild": "npm install",
		"b": "npm run build.w",
		"build": "npm run copy-deps && tsc",
		"build.w": "npm run copy-deps && tsc -w",
		"test-dev": "nyc --reporter=text --reporter=html mocha -r ts-node/register test/**/*.test.ts --exit",
		"test-dev-watch": "nodemon --watch . --ext ts --exec \"nyc --reporter=html --reporter=text mocha -r ts-node/register test/**/*.test.ts\""
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/body-parser": "^1.19.0",
		"@types/chai": "^4.2.14",
		"@types/cors": "^2.8.9",
		"@types/express": "^4.17.11",
		"@types/helmet": "0.0.48",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mocha": "^8.2.0",
		"@types/mongoose": "^5.10.3",
		"@types/multer": "^1.4.5",
		"@types/mysql": "^2.15.17",
		"@types/node": "^14.14.21",
		"@types/supertest": "^2.0.10",
		"@types/ws": "^7.4.0",
		"chai": "^4.2.0",
		"concurrently": "^5.3.0",
		"copy-newer": "^2.1.2",
		"http-proxy-middleware": "^1.0.6",
		"mocha": "^8.2.1",
		"nodemon": "^2.0.7",
		"nyc": "^15.1.0",
		"rand-token": "^1.0.1",
		"rimraf": "^3.0.2",
		"supertest": "^6.1.1",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"helmet": "^4.3.1",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"mongoose": "^5.11.12",
		"ws": "^7.4.2"
	}
}
